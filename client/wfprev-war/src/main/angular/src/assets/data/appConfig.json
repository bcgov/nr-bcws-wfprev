{
  "application": {
    "baseUrl": "#{WFPREV_BASE_URL}#",
    "environment": "#{TARGET_ENV}#"
  },

  "webade": {
    "oauth2Url": "#{WEBADE_OAUTH2_CHECK_AUTHORIZE_URL}#",
    "clientId": "WFPREV-UI",
    "authScopes": [
          "WFPREV.GET_TOPLEVEL",
          "WFPREV.GET_PREVENTION_PROJECT",
          "WFPREV.CREATE_PREVENTION_PROJECT",
          "WFPREV.UPDATE_PREVENTION_PROJECT",
          "WFPREV.DELETE_PREVENTION_PROJECT",
          "WFPREV.CREATE_PREVENTION_FISCAL",
          "WFPREV.UPDATE_PREVENTION_FISCAL",
          "WFPREV.DELETE_PREVENTION_FISCAL",
          "WFPREV.CREATE_PREVENTION_ACTIVITY",
          "WFPREV.UPDATE_PREVENTION_ACTIVITY",
          "WFPREV.DELETE_PREVENTION_ACTIVITY",
          "WFPREV.UPDATE_FINANCIAL_INFORMATION",
          "WFPREV.UPDATE_PROJECT_TRACKING",
          "WFPREV.CREATE_PERFORMANCE_UPDATE",
          "WFPREV.CREATE_SPATIAL_UPLOAD",
          "WFPREV.DELETE_SPATIAL_UPLOAD",
          "WFPREV.UPDATE_SPATIAL_METADATA",
          "WFPREV.GET_SPATIAL",
          "WFPREV.UPDATE_PROJECT_COMPLETE",
          "WFPREV.EXPORT_TO_RESULTS",
          "WFPREV.GET_RESTRICTED_FINANCIAL_DETAILS",
          "WFPREV.UPDATE_PROJECT_STATUS",
          "WFDM.*"
        ],
    "enableCheckToken": true,
    "checkTokenUrl": "#{WFPREV_CHECK_TOKEN_URL}#"
  },

   "rest": {
      "wfprev":"#{WFPREV_BASE_URL}#",
      "wfdm":"#{WFDM_BASE_URL}#",
      "openmaps":"https://openmaps.gov.bc.ca",
      "trainingAndSupportLink": "#{TRAINING_AND_SUPPORT_LINK}#"
  },

  "mapServices": {
    "geoserverApiBaseUrl": "#{GEOSERVER_API_BASE_URL}#",
    "wfnewsApiBaseUrl": "#{WFNEWS_API_BASE_URL}#",
    "wfnewsApiKey": "#{WFNEWS_API_KEY}#"
  }
}