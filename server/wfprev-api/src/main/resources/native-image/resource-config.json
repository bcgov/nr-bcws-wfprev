{
  "resources": {
    "includes": [
      {
        "pattern": "META-INF/services/org.apache.logging.log4j.spi.Provider"
      },
      {
        "pattern": "org/apache/logging/log4j/core/config/plugins/Log4j2Plugins.dat"
      },
      {
        "pattern": "org/apache/logging/log4j/core/config/xml/log4j2.xml"
      },
      {
        "pattern": "META-INF/.*.properties"
      },
      {
        "pattern": "META-INF/services/.*"
      },
      {
        "pattern": "org/hibernate/orm/.*"
      },
      {
        "pattern": "META-INF/services/org.hibernate.bytecode.spi.BytecodeProvider"
      },
      {
        "pattern": "META-INF/services/org.hibernate.boot.spi.BootstrapService"
      },
      {
        "pattern": "META-INF/services/org.hibernate.boot.model.TypeContributor"
      },
      {
        "pattern": "META-INF/services/org.jboss.jandex.IndexProvider"
      },
      {
        "pattern": "META-INF/services/io.smallrye.jandex.*"
      }
    ]
  }
}